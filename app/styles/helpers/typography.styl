h1, h2, h3, h4, h5, h6
  margin-top 20px
  margin-bottom 10px
  font-weight 500
  line-height: 1.1;

h1
  font-size 2rem

h2,h3
  font-size 1.5rem

p
  margin 0 0 10px
  line-height 1.5rem
  color rgb(34,34,34)

a
  text-decoration none
  color rgb(50,54,57)
  &:hover
    text-decoration underline
    /*color rgb(166,40,62)*/

ul, ol
  display block

b, strong
  font-weight bold